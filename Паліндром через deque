from collections import deque

def is_palindrome(s):
    cleaned = ''.join(c.lower() for c in s if c.isalnum())
    d = deque(cleaned)

    while len(d) > 1:
        if d.popleft() != d.pop():
            return False
    return True

# Ð¢ÐµÑÑ‚ÑƒÐ²Ð°Ð½Ð½Ñ
test_strings = [
    "A man a plan a canal Panama",
    "Racecar",
    "Python",
    "ÐšÐ°Ð·Ð°Ðº",
    "ÐžÐ³Ð¾"
]

for s in test_strings:
    result = is_palindrome(s)
    print(f"ðŸ” '{s}' -> {'Ð¿Ð°Ð»Ñ–Ð½Ð´Ñ€Ð¾Ð¼' if result else 'Ð½Ðµ Ð¿Ð°Ð»Ñ–Ð½Ð´Ñ€Ð¾Ð¼'}")
