import queue
import time
import random

request_queue = queue.Queue()

def generate_request(request_id):
    request = f"Request-{request_id}"
    print(f"üîÑ –ó–≥–µ–Ω–µ—Ä–æ–≤–∞–Ω–æ: {request}")
    request_queue.put(request)

def process_request():
    if not request_queue.empty():
        request = request_queue.get()
        print(f"‚úÖ –û–±—Ä–æ–±–ª–µ–Ω–æ: {request}")
    else:
        print("‚ö†Ô∏è –ß–µ—Ä–≥–∞ –ø—É—Å—Ç–∞, –Ω–µ–º–∞—î –∑–∞—è–≤–æ–∫ –¥–ª—è –æ–±—Ä–æ–±–∫–∏.")

# –°–∏–º—É–ª—è—Ü—ñ—è —Ä–æ–±–æ—Ç–∏
if __name__ == "__main__":
    request_id = 1
    while request_id <= 5:
        generate_request(request_id)
        time.sleep(0.5)
        process_request()
        request_id += 1
